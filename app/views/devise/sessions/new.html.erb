<div id="form-main">
  <div id="form-div">
    <form class="form" id="form1">

    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
       <%= render "devise/shared/error_messages", resource: resource %>

      <p class="name">
        <input name="name" type="text" class="feedback-input" placeholder="名前またはニックネーム" id="name" />
      </p>
<!-- plsceholder=最初から入れる文字 -->
      <p class="email">
        <input name="email" type="text" class=" feedback-input" id="email" placeholder="メールアドレス" />
      </p>

    

      <p class="email">
        <input type="password" type="text" class="validate[required,custom[email]] feedback-input" id="password" placeholder="パスワード" />
      </p>



      <p class="email">
        <input type="password" type="text" class="validate[required,custom[email]] feedback-input" id="password" placeholder="パスワード(確認用)" />
      </p>



      <div class="submit">
        <input type="submit" value="新規登録する" id="button-blue"/>
        <div class="ease"></div>
      </div>
      <% end %>
    </form>
  </div>
